  "authors": [
    "joaoh1"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "Apache-2.0",
  "icon": "assets/okzoomer/icon.png",

  "environment": "*",
  "entrypoints": {
