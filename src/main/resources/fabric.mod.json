    "modmenu": ["io.github.joaoh1.okzoomer.modmenu.OkZoomerModMenuApiImpl"]
  },
  "depends": {
    "autoconfig1u": ">=1.2.4",
    "cloth-config2": ">=1.8.0",
    "fabricloader": ">=0.7.0",
    "fabric": ">=0.4.0",
    "minecraft": ">=1.14.4"
